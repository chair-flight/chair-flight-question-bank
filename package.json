{
  "name": "chair-flight-question-bank",
  "repository": "chair-flight/chair-flight-question-bank",
  "version": "0.0.74",
  "description": "An open source Aviation Question bank",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "PupoSDC",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc && node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "clean": "rm -rf ./lib/",
    "prepare": "husky install && yarn build",
    "format:code": "yarn prettier . --write",
    "format:qb": "remark ./questionBank -qfo && remark ./questionBank -e mdx -u mdx -qfo && xo --fix",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.11",
    "@types/object-hash": "^2.2.1",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.4",
    "commitlint": "^16.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dedent-js": "^1.0.1",
    "gray-matter": "^4.0.3",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "prettier": "^2.5.1",
    "remark": "^14.0.2",
    "remark-cli": "^10.0.1",
    "remark-mdx": "^2.0.0-rc.2",
    "remark-preset-wooorm": "^9.1.0",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4",
    "xo": "^0.49.0",
    "zod": "^3.11.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "lint-no-html",
        false
      ],
      [
        "lint-file-extension",
        false
      ]
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "bash -c tsc",
    "*.{js,jsx,ts,tsx,html,css,json,md}": "yarn prettier --write"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}

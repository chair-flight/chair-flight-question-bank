{
  "name": "chair-flight-question-bank",
  "repository": "chair-flight/chair-flight-question-bank",
  "version": "0.1.0",
  "description": "An open source Aviation Question bank",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "PupoSDC",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "pre-build": "yarn run pre-build:los && yarn run pre-build:content && yarn run pre-build:contentTree",
    "pre-build:los": "node scripts/parseLearningObjectives.js",
    "pre-build:content": "node scripts/parseContent.js",
    "pre-build:contentTree": "node scripts/parseContentTree.js",
    "clean": "rm -rf ./lib/",
    "prepare": "husky install && yarn pre-build",
    "format": "yarn prettier . --write",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=12.0"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@open-wc/building-rollup": "^2.0.1",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.11",
    "commitizen": "^4.2.4",
    "commitlint": "^16.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dedent-js": "^1.0.1",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.14.42",
    "gray-matter": "^4.0.3",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4",
    "zod": "^3.11.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "remarkConfig": {
    "plugins": [
      "remark-frontmatter",
      "remark-mdx-frontmatter",
      "preset-wooorm",
      [
        "lint-no-html",
        false
      ],
      [
        "lint-file-extension",
        false
      ]
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "bash -c tsc",
    "*.{js,jsx,ts,tsx,html,css,json,md,mdx}": "yarn prettier --write"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "prettier": {
    "proseWrap": "always"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
